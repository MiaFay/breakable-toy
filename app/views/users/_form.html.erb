<%= form_for ([@user, @dog]) do |f| %>
  <%= f.text_field :dog_name, placeholder: "Dog Name", autofocus: true %>

  <%= f.text_field :breed, placeholder: "Breed" %>
  <%= f.text_field :sex, placeholder: "Sex" %>
  <%= f.check_box :neutered, placeholder: "Neutered" %>
  <%= f.check_box :vaccinated, placeholder: "Vaccinated" %>
  <%= f.text_field :temperment, placeholder: "Temperment" %>
  <%= f.text_field :age, placeholder: "Age" %>

  <%= f.submit%>
<% end %>
