<%= form_for([@user, @dog]) do |f| %>
  <%= f.text_field :dog_name, placeholder: "Dog Name" %>

  <%= f.text_field :breed, placeholder: "Breed" %>
  <%= f.text_field :sex, placeholder: "Sex" %>
  <%= f.boolean :neutered, placeholder: "Neutered" %>
  <%= f.boolean :vacinated, placeholder: "vacinated" %>
  <%= f.text_field :temperment, placeholder: "Temperment" %>
  <%= f.integer :age, placeholder: "Age" %>

  <%= f.submit%>
<% end %>
